{
    "proxy": null,
    "defaultProvider": "gemini",
    "providers": {
        "gemini": {
            "baseURL": "https://generativelanguage.googleapis.com/v1beta",
            "method": "POST",
            "headers": {
                "Content-Type": "application/json"
            },
            "defaultModel": "gemini-2.5-flash",
            "models": {
                "gemini-2.5-pro": {
                    "contextWindow": 2097152,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "long-context"
                    ]
                },
                "gemini-2.5-flash": {
                    "contextWindow": 1048576,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "fast"
                    ]
                }
            }
        },
        "chatgpt": {
            "baseURL": "https://api.openai.com/v1",
            "method": "POST",
            "headers": {
                "Authorization": "Bearer ${OPENAI_API_KEY}",
                "Content-Type": "application/json"
            },
            "defaultModel": "gpt-5.2-pro",
            "models": {
                "gpt-5.2-pro": {
                    "contextWindow": 256000,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "agent",
                        "top-tier"
                    ]
                },
                "gpt-5.1-codex-max": {
                    "contextWindow": 200000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning",
                        "agent"
                    ]
                },
                "gpt-5.1-codex": {
                    "contextWindow": 200000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning"
                    ]
                },
                "o3-mini-high": {
                    "contextWindow": 128000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning",
                        "fast",
                        "cheap"
                    ]
                }
            }
        },
        "claude": {
            "baseURL": "https://api.anthropic.com/v1",
            "method": "POST",
            "headers": {
                "x-api-key": "${ANTHROPIC_API_KEY}",
                "anthropic-version": "2023-06-01",
                "Content-Type": "application/json"
            },
            "defaultModel": "claude-opus-4.5",
            "models": {
                "claude-opus-4.5": {
                    "contextWindow": 200000,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "architecture",
                        "top-tier"
                    ]
                },
                "claude-sonnet-4.5": {
                    "contextWindow": 200000,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "balanced"
                    ]
                },
                "claude-3.5-haiku": {
                    "contextWindow": 200000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "fast",
                        "light"
                    ]
                }
            }
        },
        "deepseek": {
            "baseURL": "https://api.deepseek.com/v1",
            "method": "POST",
            "headers": {
                "Authorization": "Bearer ${DEEPSEEK_API_KEY}",
                "Content-Type": "application/json"
            },
            "defaultModel": "deepseek-r1",
            "models": {
                "deepseek-r1": {
                    "contextWindow": 64000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning",
                        "logic",
                        "cheap"
                    ]
                },
                "deepseek-v3": {
                    "contextWindow": 64000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning"
                    ]
                }
            }
        },
        "grok": {
            "baseURL": "https://api.x.ai/v1",
            "method": "POST",
            "headers": {
                "Authorization": "Bearer ${GROK_API_KEY}",
                "Content-Type": "application/json"
            },
            "defaultModel": "grok-3",
            "models": {
                "grok-3": {
                    "contextWindow": 128000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning",
                        "search"
                    ]
                }
            }
        },
        "ollama": {
            "baseURL": "http://localhost:11434/v1",
            "type": "local",
            "method": "POST",
            "headers": {
                "Content-Type": "application/json"
            },
            "defaultModel": "deepseek-r1:latest",
            "models": {
                "deepseek-r1:latest": {
                    "contextWindow": 8192,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning",
                        "local"
                    ]
                },
                "qwen2.5-coder:latest": {
                    "contextWindow": 32768,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning",
                        "local"
                    ]
                },
                "llama3.2:latest": {
                    "contextWindow": 8192,
                    "supportsImages": false,
                    "tags": [
                        "general",
                        "local"
                    ]
                }
            }
        }
    },
    "defaultTerminalPolicy": "turbo",
    "terminalPolicy": {
        "off": {},
        "safe": {
            "blockedCommands": [
                "rm -rf",
                "sudo",
                "dd"
            ]
        },
        "turbo": {
            "blockedCommands": [
                "rm -rf /",
                "sudo rm",
                "dd"
            ],
            "requireConfirmation": [
                "git push",
                "npm publish"
            ]
        },
        "custom": {
            "allowedCommands": [
                "npm",
                "git",
                "ls",
                "cat",
                "grep",
                "find",
                "pwd",
                "echo"
            ],
            "blockedCommands": [
                "rm -rf",
                "sudo rm",
                "dd",
                "mkfs"
            ],
            "allowedPaths": [
                "./src",
                "./tests",
                "./scripts"
            ],
            "requireConfirmation": [
                "npm install",
                "npm uninstall",
                "git push"
            ]
        }
    },
    "defaultAuthorityMode": "Autonomous",
    "authorityModes": {
        "autonomous": {
            "description": "Full Agent - Do everything until done",
            "maxIterations": 10,
            "autoTest": true,
            "autoDebug": true,
            "stopOnlyFor": [
                "data_loss_risk",
                "security_risk",
                "production_changes"
            ]
        },
        "smart": {
            "description": "AI decides when to ask based on risk",
            "riskThreshold": 3,
            "alwaysAskFor": [
                "delete_file",
                "architecture_change",
                "breaking_change"
            ],
            "neverAskFor": [
                "read_files",
                "search_code",
                "view_file"
            ]
        },
        "collaborative": {
            "description": "Always ask before executing",
            "requireApprovalFor": [
                "edit_file",
                "run_terminal",
                "delete_file"
            ]
        },
        "optimistic": {
            "description": "Execute immediately, revert if needed",
            "atomic": true,
            "autoRevert": false
        }
    },
    "rag": {
        "enabled": true,
        "autoIndex": true,
        "indexOnStartup": true,
        "maxFiles": 500,
        "topK": 3
    },
    "autoClearHistory": {
        "enabled": true,
        "mode": "smart",
        "keepSystemPrompt": true,
        "showNotification": true
    }
}
